#[Name(test)]

#[Prototype]
fn Print(str : string)

type string
{
  head : char&
}

operator [](s : string, i : uint) -> char
{
  return s.head
}

fn PrintFmt(format : string)
{
  formatIt : mut char& = format[0u]

  while (formatIt != 0)
  {
    Print("Iterating over char")
    formatIt++
  }
}

#[Entry]
fn Main() -> int
{
  PrintFmt("Hello")
}
